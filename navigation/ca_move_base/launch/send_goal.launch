<launch>
  <arg name="x"/>
  <arg name="y"/>
  <arg name="Y"/>

  <arg name="robot_id" default="$(optenv ID 1)"/>
  <arg name="ns"       default="create$(arg ns)"/>

  <node pkg="ca_move_base" type="send_goal" name="navigation_goals"
        ns="$(arg ns)" output="screen"
        args="$(arg x) $(arg y) $(arg Y)"/>
</launch>